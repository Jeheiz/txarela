<?xml version="1.0" encoding="UTF-8"?>
<definitions 
    xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
    xmlns:tns="http://www.map.es/xml-schemas"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:ns1="http://intermediacion.redsara.es/scsp/esquemas/V3/peticion"
    xmlns:ns2="http://intermediacion.redsara.es/scsp/esquemas/V3/respuesta"
    targetNamespace="http://www.map.es/xml-schemas"
    name="x53jiServicioOnlineIntermediacion">

    <types>
        <xsd:schema>
            <xsd:import namespace="http://intermediacion.redsara.es/scsp/esquemas/V3/peticion" schemaLocation="peticion.xsd"/>
            <xsd:import namespace="http://intermediacion.redsara.es/scsp/esquemas/V3/respuesta" schemaLocation="respuesta.xsd"/>
        </xsd:schema>
    </types>

    <!-- SÍNCRONO -->
    <message name="peticionSincrona">
        <part name="peticion" element="ns1:Peticion"/>
    </message>
    <message name="peticionSincronaResponse">
        <part name="respuesta" element="ns2:Respuesta"/>
    </message>

    <!-- ASÍNCRONO -->
    <message name="peticionAsincrona">
        <part name="peticion" element="ns1:Peticion"/>
    </message>
    <message name="peticionAsincronaResponse">
        <part name="respuesta" element="ns2:Respuesta"/>
    </message>

    <message name="consultarPeticionAsincrona">
        <part name="peticion" element="ns1:Peticion"/>
    </message>
    <message name="consultarPeticionAsincronaResponse">
        <part name="respuesta" element="ns2:Respuesta"/>
    </message>

    <!-- PORTS -->
    <portType name="IntermediacionOnlinePortType">
        <operation name="peticionSincrona">
            <input message="tns:peticionSincrona"/>
            <output message="tns:peticionSincronaResponse"/>
        </operation>
    </portType>

    <portType name="IntermediacionOnlineAsyncPortType">
        <operation name="peticionAsincrona">
            <input message="tns:peticionAsincrona"/>
            <output message="tns:peticionAsincronaResponse"/>
        </operation>
        <operation name="consultarPeticionAsincrona">
            <input message="tns:consultarPeticionAsincrona"/>
            <output message="tns:consultarPeticionAsincronaResponse"/>
        </operation>
    </portType>

    <!-- BINDINGS -->
    <binding name="IntermediacionOnlinePortBinding" type="tns:IntermediacionOnlinePortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="peticionSincrona">
            <soap:operation soapAction="peticionSincrona"/>
            <input><soap:body use="literal"/></input>
            <output><soap:body use="literal"/></output>
        </operation>
    </binding>

    <binding name="IntermediacionOnlineAsyncPortBinding" type="tns:IntermediacionOnlineAsyncPortType">
        <soap:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
        <operation name="peticionAsincrona">
            <soap:operation soapAction="peticionAsincrona"/>
            <input><soap:body use="literal"/></input>
            <output><soap:body use="literal"/></output>
        </operation>
        <operation name="consultarPeticionAsincrona">
            <soap:operation soapAction="consultarPeticionAsincrona"/>
            <input><soap:body use="literal"/></input>
            <output><soap:body use="literal"/></output>
        </operation>
    </binding>

    <!-- SERVICES -->
    <service name="x53jiServicioOnlineIntermediacion">
        <port name="IntermediacionOnlinePort" binding="tns:IntermediacionOnlinePortBinding">
            <soap:address location="http://localhost:8080/ittxartela/online"/>
        </port>
        <port name="IntermediacionOnlineAsyncPort" binding="tns:IntermediacionOnlineAsyncPortBinding">
            <soap:address location="http://localhost:8080/ittxartela/online/async"/>
        </port>
    </service>
</definitions>
